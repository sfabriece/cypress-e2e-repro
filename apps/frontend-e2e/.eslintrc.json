{
  "extends": ["plugin:cypress/recommended", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "sonarjs/no-duplicate-string": "off",
        "unicorn/prefer-module": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "unicorn/numeric-separators-style": "off"
      }
    }
  ]
}
